<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="WeatherApp.MainWindow"
        Title="WeatherApp">
        <StackPanel>
            <TextBlock Name="WeatherLabela" FontSize="20" Text="Weather Info" HorizontalAlignment="Center" Margin="10"/>
            <TextBlock Name="TemperatureLabela" FontSize="16" HorizontalAlignment="Center" Margin="10"/>
            <TextBlock Name="HumidityLabela" FontSize="16" HorizontalAlignment="Center" Margin="10"/>
            <TextBlock Name="CityNamea" FontSize="16" HorizontalAlignment="Center" Margin="10"/>
            <TextBlock Name="Lat_longa" FontSize="16" HorizontalAlignment="Center" Margin="10"/>
            <TextBox Name="csa" Width="200" Margin="10"/>
        <Button Content="Fetch Weather" HorizontalAlignment="Center" Margin="10" Click="Button_Click"/>
        <TextBlock Name="TaZonzSeTexa" FontSize="16"  Margin="110"/>
    </StackPanel>
</Window>
